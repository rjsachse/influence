<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="musicvideos">
    <name>Recently Played Music Videos</name>
    <match>all</match>
    <rule field="lastplayed" operator="inthelast">8 weeks</rule>
    <limit>20</limit>
    <order direction="descending">lastplayed</order>
</smartplaylist>
