<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="songs">
    <name>Recently Played Music</name>
    <match>all</match>
    <rule field="lastplayed" operator="inthelast">8 weeks</rule>
    <limit>60</limit>
    <order direction="descending">lastplayed</order>
</smartplaylist>
