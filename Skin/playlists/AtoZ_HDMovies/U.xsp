<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>U</name>
    <match>all</match>
    <rule field="videoresolution" operator="greaterthan">710</rule>
	<rule field="title" operator="startswith">U</rule>
	<rule field="title" operator="startswith">u</rule>
    <order direction="ascending">title</order>
</smartplaylist>
