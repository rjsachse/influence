<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>Recently Played Movies</name>
    <match>all</match>
    <rule field="lastplayed" operator="inthelast">8 weeks</rule>
    <limit>50</limit>
    <order direction="descending">lastplayed</order>
</smartplaylist>
